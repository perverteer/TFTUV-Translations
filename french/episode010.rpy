    keo "Merci, [p_name]."

# game/episode010.rpy:1088
translate french ep010_erylin_keo_sex_proposal_a70d8747:

    # er "Why don’t you be a dear and clean up the dishes."
    er "Pourquoi ne serais-tu pas un ange et ne nettoierais-tu pas la vaisselle ?"

# game/episode010.rpy:1090
translate french ep010_erylin_keo_sex_proposal_775c11df:

    # keo "Of course, mother."
    keo "Bien sûr, maman."

# game/episode010.rpy:1092
translate french ep010_erylin_keo_sex_proposal_c98c9560:

